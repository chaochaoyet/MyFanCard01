<AbsoluteLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:id="@+id/widget0">
    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_x="275dp"
        android:layout_y="5dp"
        android:text="@string/toSearch"
        android:id="@+id/search_button"></Button>
    <EditText
        android:layout_width="150dp"
        android:layout_height="45dp"
        android:textSize="19dp"
        android:layout_x="125dp"
        android:layout_y="4dp"
        android:background="@drawable/shape"
        android:id="@+id/input_key_edittext"></EditText>
    <TextView
        android:id="@+id/showin_textview"
        android:layout_width="120dp"
        android:layout_height="29dp"
        android:textSize="20dp"
        android:layout_x="5dp"
        android:layout_y="16dp"
        android:text="@string/showInput"></TextView>
    <TextView
        android:layout_height="370dp"
        android:layout_width="fill_parent"
        android:layout_x="0dp"
        android:layout_y="60dp"
        android:id="@+id/result_textview"></TextView>
    </AbsoluteLayout>